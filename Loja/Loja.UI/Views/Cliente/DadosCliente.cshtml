        }
        else {
            if ($('#enderecoEntrega').css("readonly", true)) {
                $('#enderecoEntrega').prop("readonly", false);
            }
        }
        if ($('#compEntrega').val() != "") {
            $("#lblComplementoEntrega").addClass('active');
        }
        if ($('#numEntrega').val() != "") {
